<!DOCTYPE html>
<html>
  <head>
    <title>Aircart</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link href='http://fonts.googleapis.com/css?family=Lato:300,400,300italic,400italic' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Montserrat:400,700' rael='stylesheet' type='text/css'>

    <link rel="stylesheet" href="/assets/normalize.css">

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap-theme.min.css" integrity="sha384-fLW2N01lMqjakBkx3l/M9EahuwpSfeNvV63J5ezn3uZzapT0u7EYsXMjQV+0En5r" crossorigin="anonymous">
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js" integrity="sha384-0mSbJDEHialfmuBBQP6A4Qrprq5OVfW37PRR3j5ELqxss1yVqOtnepnHVP9aJ7xS" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">

    <link rel="stylesheet" href="/assets/home.scss">
    <link rel="stylesheet" href="/assets/main.css">

    <link rel="stylesheet" href="/assets/animate.css">
    <link rel="stylesheet" href="/assets/flexslider.css">
    <!-- Theme CSS -->
    <link id="theme-style" rel="stylesheet" href="/assets/styles.css">
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]> <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script> <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script> <![endif]-->

  </head>
  <body class="home-page">
    <div class="wrapper">
      <!-- ******HEADER****** -->
      <header id="header" class="header">
        <div class="container">
            <%= link_to image_tag("/assets/aircart.png"), root_path, :class => "navbar-brand"  %>
          <!--//logo-->
          <nav id="main-nav" class="main-nav navbar-right" role="navigation">
            <div class="navbar-header">
              <button class="navbar-toggle" type="button" data-toggle="collapse" data-target="#navbar-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
              </button>
              <!--//nav-toggle-->
            </div>
            <!--//navbar-header-->
            <div class="navbar-collapse collapse" id="navbar-collapse">
              <ul class="nav navbar-nav">
                <li class="active nav-item">
                  <%= link_to "Home", root_path %>
                </li>
                <li class="nav-item">
                  <%= link_to "About", about_path %>
                </li>
                <li class="nav-item">
                  <%= link_to "FAQ", about_path %>
                </li>
                <li class="nav-item">
                  <%= link_to "Browse Requests", posts_path %>
                </li>
                <li class="nav-item">
                  <%= link_to "Post a Request", posts_new_path %>
                </li>
                <li class="nav-item">
                  <%= link_to "Contact Us", new_contact_path %>
                </li>
                <!-- <li class="nav-item"><button type="button" class="login-trigger btn btn-link" data-toggle="modal" data-target="#login-modal">Log in</button></li>
                            <li class="nav-item nav-item-cta last"><button type="button" class="btn btn-cta btn-cta-primary" data-toggle="modal" data-target="#signup-modal" >Get Started</button></li> -->
                <% if current_user %>
                <%= link_to "Sign Out", destroy_user_session_path, method: :delete, class: "nav-item login-trigger btn btn-link" %>
              <% else %>
                <%= link_to "Log In", new_user_session_path, class: "nav-item btn btn-cta btn-cta-primary", method: :get %>
                <%= link_to "Sign Up", new_user_registration_path, class: "nav-item btn btn-cta btn-cta-primary", method: :get %>
                <% end %>
              </ul>
              <!--//nav-->
            </div>
            <!--//navabr-collapse-->
          </nav>
          <!--//main-nav-->
        </div>
        <!--//container-->
      </header>
      <!--//header-->

      <% flash.each do |key, value| %>
      <%= content_tag :div, value, class: "alert alert-#{key}" %>
      <% end %>

      <%= yield %>
      <!-- Javascript -->
      <script type="text/javascript" src="https://code.jquery.com/jquery-1.12.1.min.js"></script>
      <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/flexslider/2.6.0/flexslider.min.css"></script>

      <!-- Vimeo video API -->
      <script src="http://a.vimeocdn.com/js/froogaloop2.min.js"></script>
    </div>
  </body>
</html>
